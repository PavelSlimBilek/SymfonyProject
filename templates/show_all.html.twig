{# show_all.html.twig #}

<!DOCTYPE html>
<html lang="en">
<head>
    <title>Show All Data</title>
</head>
<body>
<h1>All Data</h1>
{% if entities %}
    <ul>
        {% for entity in entities %}
            <li>{{ entity.name }}</li>
        {% endfor %}
    </ul>
{% else %}
    <p>No data available.</p>
{% endif %}
</body>
</html>